    "cldr-data": "~30.0.2",
    "tslib": "~1.8.1"
  },
  "devDependencies": {
    "@dojo/cli-build-app": "^4.0.0",
    "@dojo/cli-test-intern": "^4.0.0",
    "@types/glob": "~5.0.0",
